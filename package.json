{
  "name": "tmu-times",
  "version": "1.0.0",
  "description": "University News & Social Platform - X/Twitter style app for campus communities",
  "author": "TMU Development Team",
  "private": true,
  "homepage": "https://JESamm.github.io/tmu-times",
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
